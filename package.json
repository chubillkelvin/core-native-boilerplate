{
  "name": "core-native-boilerplate",
  "version": "1.0.7",
  "description": "A boilerplate project for the core-native library.",
  "author": "Kelvin Chu <chubillkelvin@gmail.com> (github.com/chubillkelvin)",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "react",
    "react-native",
    "redux",
    "redux-saga",
    "typescript",
    "boilerplate",
    "core-native",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chubillkelvin/core-native-boilerplate.git"
  },
  "scripts": {
    "ts-node": "TS_NODE_PROJECT=./script/tsconfig.json ./node_modules/.bin/ts-node",
    "android": "react-native run-android",
    "check": "yarn ts-node script/check.ts",
    "debug": "open rndebugger://set-debugger-loc?host=localhost&port=8081",
    "format": "yarn ts-node script/format.ts",
    "ios": "react-native run-ios",
    "module": "yarn ts-node script/module.ts",
    "native": "bash ./script/native.sh",
    "start": "react-native start --reset-cache"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "5.8.10",
    "@react-navigation/stack": "5.12.8",
    "core-native": "2.4.0",
    "react-native": "0.64.1",
    "react-native-device-info": "7.3.1",
    "react-native-gesture-handler": "1.9.0",
    "react-native-reanimated": "1.13.2",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "2.16.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-v8": "0.63.3-patch.0",
    "v8-android": "8.84.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/runtime": "7.12.5",
    "@pinnacle0/eslint-plugin": "0.1.0",
    "@types/chalk": "2.2.0",
    "@types/fs-extra": "9.0.5",
    "@types/yargs": "15.0.12",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "axios": "0.21.2",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "4.1.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "fs-extra": "9.0.1",
    "jetifier": "1.6.6",
    "metro-react-native-babel-preset": "0.64.0",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "yargs": "16.2.0"
  },
  "resolutions": {
    "@react-native-community/cli": "4.9.0"
  },
  "engines": {
    "node": ">= 12.16"
  }
}
